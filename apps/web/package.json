{
  "name": "@lazycuh/cloudy-clip-web",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "./scripts/build.sh",
    "release": "./scripts/deploy.sh",
    "check-code-styles": "prettier  --write --check \"**/(*.ts|*.html|*.scss|*.json)\"",
    "lint": "eslint \"src/**/*.{ts,html}\"",
    "localize": "ng extract-i18n && node ./scripts/merge-localization-messages.js",
    "ng": "ng",
    "test": "vitest run --config vitest/vite.config.ts",
    "test:ci": "vitest run --config vitest/vite.config.ts --coverage",
    "test:ci:no-coverage": "vitest run --config vitest/vite.config.ts",
    "test:local:all": "vitest run --config vitest/vite.config.ts",
    "test:watch": "vitest --config vitest/vite.config.ts",
    "serve:ssr:web": "node dist/web/server/server.mjs",
    "start": "ng serve --configuration=en",
    "start:es": "ng serve --configuration=es",
    "start:vi": "ng serve --configuration=vi",
    "http-server": "http-server --ssl --proxy http://localhost:8787 --cert keystore/cloudy-clip-cert.pem --key keystore/cloudy-clip-key.pem --port 4300 dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.6",
    "@angular/cdk": "^19.2.9",
    "@angular/common": "^19.2.6",
    "@angular/compiler": "^19.2.6",
    "@angular/core": "^19.2.6",
    "@angular/forms": "^19.2.6",
    "@angular/material": "^19.2.9",
    "@angular/platform-browser": "^19.2.6",
    "@angular/platform-browser-dynamic": "^19.2.6",
    "@angular/platform-server": "^19.2.6",
    "@angular/router": "^19.2.6",
    "@angular/ssr": "^19.2.7",
    "@lazycuh/angular-anchored-floating-box": "^3.0.2",
    "@lazycuh/angular-confirmation-capture": "^2.0.0",
    "@lazycuh/angular-notification": "^2.0.1",
    "@lazycuh/angular-tooltip": "^3.0.1",
    "@lazycuh/http": "workspace:*",
    "@lazycuh/execute-until": "^3.0.1",
    "@lazycuh/logging": "workspace:*",
    "@lazycuh/optional": "workspace:*",
    "@lazycuh/web-ui-common": "workspace:*",
    "@stripe/stripe-js": "^4.8.0",
    "@tiptap/core": "^2.11.7",
    "@tiptap/extension-bubble-menu": "^2.11.7",
    "@tiptap/extension-highlight": "^2.11.7",
    "@tiptap/extension-link": "^2.11.7",
    "@tiptap/extension-table": "^2.11.7",
    "@tiptap/extension-table-cell": "^2.11.7",
    "@tiptap/extension-table-header": "^2.11.7",
    "@tiptap/extension-table-row": "^2.11.7",
    "@tiptap/extension-task-item": "^2.11.7",
    "@tiptap/extension-task-list": "^2.11.7",
    "@tiptap/extension-text-align": "^2.11.7",
    "@tiptap/pm": "^2.11.7",
    "@tiptap/starter-kit": "^2.11.7",
    "bezier-easing": "^2.1.0",
    "currency.js": "^2.0.4",
    "express": "^4.18.2",
    "ngx-skeleton-loader": "^10.0.0",
    "ngx-tiptap": "^12.0.0",
    "rxjs": "^7.8.2",
    "tippy.js": "^6.3.7",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.15.1",
    "@analogjs/vite-plugin-angular": "^1.15.1",
    "@analogjs/vitest-angular": "^1.15.1",
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.6",
    "@angular/localize": "^19.2.6",
    "@lazycuh/tsconfig-base": "workspace:*",
    "@lazycuh/types": "workspace:*",
    "@nx/vite": "^20.7.2",
    "@testing-library/angular": "^17.3.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.17",
    "@vitest/coverage-istanbul": "3.1.1",
    "angular-eslint": "^19.3.0",
    "happy-dom": "^17.4.4",
    "msw": "2.4.11",
    "terser": "^5.39.0",
    "typescript": "~5.6.3",
    "vite": "^6.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "whatwg-fetch": "^3.6.20"
  }
}
